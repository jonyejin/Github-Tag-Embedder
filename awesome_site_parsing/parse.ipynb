   "source": [
    "import json\n",
    "file_path = \"key_as_space.json\"\n",
    "\n",
    "# Opening JSON file\n",
    "f = open('test.json')  \n",
    "awesome_file = json.load(f)\n",
    "my_json = {}\n",
    "\n",
    "for key,values in awesome_file.items():\n",
    "  my_json[key.replace('-', ' ')] = values\n",
    "\n",
    "with open(file_path, 'w') as outfile:\n",
    "    json.dump(my_json, outfile)"
   ]
  }
